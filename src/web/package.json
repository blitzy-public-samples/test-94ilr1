    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lighthouse": "^11.0.0",
    "nock": "^13.3.8",
    "prettier": "^2.8.8",
    "sonarqube-scanner": "^3.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vite-plugin-bundle-analyzer": "^0.8.1",
