    "js-base64": "^3.7.5",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "opossum": "^7.1.0",
    "qs": "^6.11.0",
    "quick-lru": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
